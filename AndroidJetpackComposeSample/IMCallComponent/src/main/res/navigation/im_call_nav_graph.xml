<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/im_call_nav_graph"
    app:startDestination="@id/video_call_fragment">


    <fragment
        android:id="@+id/video_call_fragment"
        android:name="com.nxg.im.call.component.VideoCallFragment"
        android:label="视频通话">

        <argument
            android:name="uuid"
            android:defaultValue="0L"
            app:argType="long" />

        <deepLink app:uri="android-app://com.nxg.app/video_call_fragment/{uuid}" />

    </fragment>

    <fragment
        android:id="@+id/LiveStreamingFragment"
        android:name="com.nxg.webrtcmobile.livestreaming.LiveStreamingFragment"
        android:label="视频通话">

        <deepLink app:uri="android-app://com.nxg.app/LiveStreamingFragment" />

    </fragment>

</navigation>